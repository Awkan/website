<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="fr"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="fr"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="fr"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="fr"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <title>AFSY - Association Francophone des Utilisateurs de Symfony {% block page_title %}{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="L'AFSY est un groupe d'utilisateurs francophones du framework qui a pour objectif de promouvoir l'usage de Symfony en milieu professionnel et universitaire">
        <meta name="author" content="">

        <link rel="shortcut icon" href="/favicon.ico"/>
        <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
          <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->

        {% stylesheets
            'bundles/afsyfront/css/bootstrap.css'
            'bundles/afsyfront/css/bootstrap-responsive.css'
            'bundles/afsyfront/css/style.css'
            filter='cssrewrite,yui_css' output='css/compiled/main.css'
            output='css/afsy.css' %}
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
        {% endstylesheets %}

        <link rel="alternate" type="application/atom+xml" title="AFSY - tous les articles" href="{{ url('feed_atom') }}"/>
    </head>
    <body>
        <div class="container">
            {% include 'AfsyFrontBundle::header.html.twig' %}

            {% block body %}{% endblock %}
        </div>

        {% block footer %}
            {% include 'AfsyFrontBundle::footer.html.twig' %}
        {% endblock %}
    </body>
</html>
